    "category": { "pattern": "^([A-Za-z]{4})?$", "default": "" },
    "type": { "pattern": "^[TCO]?$", "default": "" },
    "audience": { "pattern": "^[CE]?$", "default": "E" },
    "search": { "pattern": "", "default": "" },
    "pageColor": { "pattern": "^[f0]{3}$", "default": "fff" },
    "textColor": { "pattern": "^[f0]{3}$", "default": "000" },
    "voice": { "pattern": "^(silent|male|female|child)$", "default": "silent" },
    "locale": { "pattern": "^[a-z]{2,3}$", "default": "en" },
    "favorites": { "pattern": "(^[AR]?\\d+(,+\\d+)*$)|(^$)", "default": "" },
    "fpage": { "pattern": "^\\d+$", "default": "1" },
    "collection": { "pattern": "^[-\\w\\d]*$", "default": "" },
